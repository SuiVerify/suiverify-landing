{
  "rewrites": [
    {
      "source": "/auth",
      "destination": "https://suiverify-main.vercel.app/auth"
    },
    {
      "source": "/dashboard",
      "destination": "https://suiverify-main.vercel.app/dashboard"
    },
    {
      "source": "/kyc",
      "destination": "https://suiverify-main.vercel.app/kyc"
    },
    {
      "source": "/admin",
      "destination": "https://suiverify-main.vercel.app/admin"
    },
    {
      "source": "/adminLogin",
      "destination": "https://suiverify-main.vercel.app/adminLogin"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://suiverify-main.vercel.app/api/$1"
    }
  ]
}
